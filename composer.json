{
    "name": "becklyn/cron-job",
    "description": "Centralized cron job integration for Symfony apps.",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/cron-job-bundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/doctrine-bundle": "^1.11 || ^2.0",
        "doctrine/orm": "^2.6",
        "dragonmantank/cron-expression": "^2.3",
        "psr/log": "^1.1",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/lock": "^4.4 || ^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\CronJobBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\CronJobBundle\\": "tests/"
        }
    },
    "bin": [
        "bin/cron"
    ],
    "config": {
        "sort-packages": true
    }
}
