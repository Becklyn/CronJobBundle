{
    "name": "becklyn/cron-job",
    "description": "Centralized cron job integration for Symfony apps.",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/cron-job-bundle",
    "require": {
        "php": ">=7.4",
        "doctrine/doctrine-bundle": "^1.12.13 || ^2.5.0",
        "doctrine/orm": "^2.11.1",
        "dragonmantank/cron-expression": "^v2.3.1",
        "psr/log": "^3.0.0",
        "sentry/sentry-symfony": "^4.2.6",
        "symfony/console": "^v5.4.3 || ^v6.0.3",
        "symfony/framework-bundle": "^v5.4.3 || ^v6.0.3",
        "symfony/http-kernel": "^v5.4.3 || ^v6.0.3",
        "symfony/lock": "^v5.4.3 || ^v6.0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^v5.4.3 || ^v6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\CronJobBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\CronJobBundle\\": "tests/"
        }
    },
    "bin": [
        "bin/cron"
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    }
}
